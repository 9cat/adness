<% layout('sblayout') %>

<div class="content-page col-sm-12">
<% if (auctionsOpen.length > 0) { %>

  <% include _registration_status %>
  <% auctionsOpen.forEach(function(auction) { %>
    <% include auctions/index_show %>
  <% }) %>
<% } else { %>

  <h3>
    There are no open auctions today. Check back later.
  </h3>
</div>

<% } %>
